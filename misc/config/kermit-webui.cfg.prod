[webui]
base_url=
rest_server_url=http://localhost:4567/mcollective/
rest_server_ping_url=http://locahost:4567/
rest_server_scheduler=http://localhost:4567/schedule/
rest_server_scheduler_status=http://localhost:4567/schedstatus/
rest_server_scheduler_output=http://localhost:4567/schedoutput/
amqp_receive_inventory_folder=/var/lib/kermit/queue/kermit.inventory
amqp_receive_log_folder=/var/lib/kermit/queue/kermit.log
static_file_location=/var/www/kermit-webui/
templates_location=/usr/share/kermit-webui/templates/
fixtures_location=/etc/kermit/webui/fixtures/
authentication=default
environment.level=2
filters.server=false
filters.class=true
filter.agent=false
filter.action=false
alerting.from_mail=no_reply@kermit.fr
email.host=localhost
email.port=25
email.username=
email.password=
email.usetls=False
admin.area.title=Kermit Admin Area
levels.number=5
show_scheduler=false
show_status_bar=True

[webui-database]
driver=django.db.backends.sqlite3
name=/var/lib/kermit/webui/db/sqlite.db
host=
port=
user=
password=

#Sample PostgreSQL configuration
#driver=postgresql_psycopg2
#name=kermit
#host=
#port=
#user=kermit
#password=kermit

[webui_logs]
main.file=/var/log/kermit/kermit-webui.log
main.level=INFO
calls.file=/var/log/kermit/kermit-mcollective-calls.log
calls.level=DEBUG

[hiera]
redis_server=localhost
redis_port = 6379
redis_database = 0
redis_password = password 

[puppet]
puppetmaster_hostname=puppet

[saml2]
xmlsec.binary=/usr/bin/xmlsec1
entityId=AutomatixD1
serviceprovider.name="Automatix Service Provider"
serviceprovider.url.login=http://npplam01.preprod.org/automatix/auth/saml2/SSO/
serviceprovider.url.logout=http://npplam01.preprod.org/automatix/saml2/logout/
idp.url.login=https://preprod.idpdecathlon.oxylane.com:9031/idp/SSO.saml2
idp.url.logout=https://preprod.idpdecathlon.oxylane.com:9031/idp/SLO.saml2


